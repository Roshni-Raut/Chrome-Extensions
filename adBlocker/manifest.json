{
    "name":"ad blocker",
    "description":" undetectable ad blocker",
    "manifest_version":2,
    "version":"1.0.0",
    "background":{
        "scripts":["background.js"],
        "persisten":true
    },
    "permissions":[
        "storage",
        "webRequest",
        "activeTab",
        "webRequestBlocking",
        "http://*/",
        "https://*/"
    ],
    "icons":{
        "16":"./icons/logo16.png",
        "48":"./icons/logo48.png",
        "128":"./icons/logo128.png"
    },
    "browser_action":{
        "default_title":"adblocker",
        "default_popup":"block.html"
    }
}